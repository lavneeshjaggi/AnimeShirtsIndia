{
  "version": 2,
  "outputDirectory": "client/dist",
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/login", "dest": "server.js" },
    { "src": "/logout", "dest": "server.js" },
    { "src": "/register", "dest": "server.js" },
    { "src": "/authenticated", "dest": "server.js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
